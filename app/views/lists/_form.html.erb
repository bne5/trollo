<h1>Add List</h1>
<%= form_with(model: [@board, @list], local: true, class: "ui form" ) do |f| %>
  <%= f.label :user %>
  <%= f.select :user_id, @users.collect { |u| [u.full_name, u.id] } %>
  <br />
  <%= f.label :role %>
  <%= f.select :item %>
  <br />
  <%= f.submit nil, class: "ui green button inverted" %>
<% end %>